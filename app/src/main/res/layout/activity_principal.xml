<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="isiasromero.fca.PRINCIPAL"
    android:background="#FFFFFFFF">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonMaquinarias"
        android:src="@drawable/ma"
        android:background="#FFFFFFFF"
        android:layout_alignTop="@+id/imageButtonPLANTAS"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonInsectos"
        android:src="@drawable/in"
        android:background="#FFFFFFFF"
        android:layout_below="@+id/imageButtonMaquinarias"
        android:layout_alignParentStart="true"
        android:layout_marginTop="92dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonPLANTAS"
        android:src="@drawable/icono"
        android:background="#FFFFFFFF"
        android:layout_marginTop="53dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/textView6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="PLANTAS"
        android:id="@+id/textView6"
        android:layout_below="@+id/imageButtonMaquinarias"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/imageButtonInsectos"
        android:textColor="@color/abc_input_method_navigation_guard"
        android:textAlignment="center" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="MAQUINARIA"
        android:id="@+id/textView7"
        android:layout_below="@+id/imageButtonMaquinarias"
        android:layout_alignEnd="@+id/imageButtonMaquinarias"
        android:textColor="@color/abc_input_method_navigation_guard"
        android:textAlignment="center" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="INSECTOS"
        android:id="@+id/textView8"
        android:layout_below="@+id/imageButtonInsectos"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/imageButtonInsectos"
        android:textColor="@color/abc_input_method_navigation_guard"
        android:textAlignment="center" />

</RelativeLayout>
